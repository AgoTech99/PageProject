<Window x:Class="PageProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PageProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="Beige">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <Button  Content="Click" Width="100" Height="50"  Click="Button_Click" BorderBrush="Blue"  Background="Aqua">
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="20"/>
                </Style>
            </Button.Resources>
            <Button.Triggers >

            </Button.Triggers>
        </Button>
    </StackPanel>
</Window>
